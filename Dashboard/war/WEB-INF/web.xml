<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	<servlet>
        <servlet-name>login</servlet-name>
        <servlet-class>dashboard.servlet.LoginServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>login</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>registration</servlet-name>
        <servlet-class>dashboard.servlet.RegistrationServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>registration</servlet-name>
        <url-pattern>/register</url-pattern>
    </servlet-mapping>
 	<servlet>
     	<servlet-name>track</servlet-name>
     	<servlet-class>dashboard.servlet.TrackingServlet</servlet-class>
 	</servlet>
 	<servlet-mapping>
 	    <servlet-name>track</servlet-name>
 	    <url-pattern>/track</url-pattern>
 	</servlet-mapping>
 	<servlet>
 	    <servlet-name>logout</servlet-name>
 	    <servlet-class>dashboard.servlet.LogoutServlet</servlet-class>
 	</servlet>
 	<servlet-mapping>
 	    <servlet-name>logout</servlet-name>
 	    <url-pattern>/logout</url-pattern>
 	</servlet-mapping>
 	<servlet>
        <servlet-name>stats</servlet-name>
        <servlet-class>dashboard.servlet.statServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>stats</servlet-name>
        <url-pattern>/stats</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>settings</servlet-name>
        <servlet-class>dashboard.servlet.SettingServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>settings</servlet-name>
        <url-pattern>/settings</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>achievements</servlet-name>
        <servlet-class>dashboard.servlet.AchievementServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>achievements</servlet-name>
        <url-pattern>/achievements</url-pattern>
    </servlet-mapping>
    <servlet>
        <servlet-name>feed</servlet-name>
        <servlet-class>dashboard.servlet.LiveFeedServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>feed</servlet-name>
        <url-pattern>/feed</url-pattern>
    </servlet-mapping>
    <welcome-file-list>
        <welcome-file>login</welcome-file>
    </welcome-file-list>
 	<servlet>
 		<servlet-name>SystemServiceServlet</servlet-name>
  		<servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  		<init-param>
   			<param-name>services</param-name>
   			<param-value/>
  		</init-param>
 	</servlet>
 	<servlet-mapping>
  		<servlet-name>SystemServiceServlet</servlet-name>
  		<url-pattern>/_ah/spi/*</url-pattern>
 	</servlet-mapping>
</web-app>